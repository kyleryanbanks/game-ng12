{
  "version": 1,
  "projects": {
    "controller": {
      "projectType": "library",
      "root": "libs/controller",
      "sourceRoot": "libs/controller/src",
      "prefix": "game-ng12",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/controller"],
          "options": {
            "jestConfig": "libs/controller/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/controller/src/**/*.ts",
              "libs/controller/src/**/*.html"
            ]
          }
        }
      }
    },
    "familiar": {
      "projectType": "application",
      "root": "apps/familiar",
      "sourceRoot": "apps/familiar/src",
      "prefix": "game-ng12",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/familiar",
            "index": "apps/familiar/src/index.html",
            "main": "apps/familiar/src/main.ts",
            "polyfills": "apps/familiar/src/polyfills.ts",
            "tsConfig": "apps/familiar/tsconfig.app.json",
            "assets": [
              "apps/familiar/src/favicon.ico",
              "apps/familiar/src/assets"
            ],
            "styles": ["apps/familiar/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/familiar/src/environments/environment.ts",
                  "with": "apps/familiar/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "familiar:build:production"
            },
            "development": {
              "browserTarget": "familiar:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/familiar/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "familiar:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/familiar/src/**/*.ts",
              "apps/familiar/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/familiar"],
          "options": {
            "jestConfig": "apps/familiar/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "familiar-e2e": {
      "root": "apps/familiar-e2e",
      "sourceRoot": "apps/familiar-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/familiar-e2e/cypress.json",
            "devServerTarget": "familiar:serve:development",
            "tsConfig": "apps/familiar-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "familiar:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/familiar-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "game-loop": {
      "projectType": "library",
      "root": "libs/game-loop",
      "sourceRoot": "libs/game-loop/src",
      "prefix": "game-ng12",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/game-loop"],
          "options": {
            "jestConfig": "libs/game-loop/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/game-loop/src/**/*.ts",
              "libs/game-loop/src/**/*.html"
            ]
          }
        }
      }
    },
    "input-viewer-shell": {
      "projectType": "library",
      "root": "libs/input-viewer/shell",
      "sourceRoot": "libs/input-viewer/shell/src",
      "prefix": "view",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/input-viewer/shell"],
          "options": {
            "jestConfig": "libs/input-viewer/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/input-viewer/shell/src/**/*.ts",
              "libs/input-viewer/shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "roost": {
      "root": "apps/roost",
      "sourceRoot": "apps/roost/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "nx-electron:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/roost",
            "main": "apps/roost/src/main.ts",
            "tsConfig": "apps/roost/tsconfig.app.json",
            "assets": ["apps/roost/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/roost/src/environments/environment.ts",
                  "with": "apps/roost/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "nx-electron:execute",
          "options": {
            "buildTarget": "roost:build"
          }
        },
        "package": {
          "builder": "nx-electron:package",
          "options": {
            "name": "roost",
            "frontendProject": "familiar",
            "outputPath": "dist/packages",
            "prepackageOnly": true
          }
        },
        "make": {
          "builder": "nx-electron:make",
          "options": {
            "name": "roost",
            "frontendProject": "familiar",
            "outputPath": "dist/executables"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/roost/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/roost"],
          "options": {
            "jestConfig": "apps/roost/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sequencer-data": {
      "projectType": "library",
      "root": "libs/sequencer/data",
      "sourceRoot": "libs/sequencer/data/src",
      "prefix": "game-ng12",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sequencer/data"],
          "options": {
            "jestConfig": "libs/sequencer/data/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/sequencer/data/src/**/*.ts",
              "libs/sequencer/data/src/**/*.html"
            ]
          }
        }
      }
    },
    "sequencer-shell": {
      "projectType": "library",
      "root": "libs/sequencer/shell",
      "sourceRoot": "libs/sequencer/shell/src",
      "prefix": "seq",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sequencer/shell"],
          "options": {
            "jestConfig": "libs/sequencer/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/sequencer/shell/src/**/*.ts",
              "libs/sequencer/shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "shell": {
      "projectType": "library",
      "root": "libs/shell",
      "sourceRoot": "libs/shell/src",
      "prefix": "game-ng12",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shell"],
          "options": {
            "jestConfig": "libs/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shell/src/**/*.ts",
              "libs/shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "fam",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "familiar"
}
