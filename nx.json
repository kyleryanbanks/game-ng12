{
  "npmScope": "game-ng12",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "controller-data": {
      "tags": ["scope:familiar", "type:data"]
    },
    "controller-shared": {
      "tags": ["scope:[roost", "familiar]", "type:[shared]"]
    },
    "familiar": {
      "tags": []
    },
    "familiar-e2e": {
      "tags": [],
      "implicitDependencies": ["familiar"]
    },
    "familiar-shell": {
      "tags": ["scope:familiar", "type:shell"]
    },
    "game-loop": {
      "tags": ["scope:game-loop", "type:service"]
    },
    "input-viewer-shell": {
      "tags": ["scope:familiar", "type:shell"]
    },
    "input-viewer-ui": {
      "tags": ["scope:familiar", "type:ui"]
    },
    "recorder-data": {
      "tags": ["scope:familiar", "type:data"]
    },
    "roost": {},
    "sequencer-data": {
      "tags": ["scope:familiar", "type:data"]
    },
    "sequencer-shell": {
      "tags": ["scope:familiar", "type:shell"]
    },
    "ui": {
      "tags": ["scope:familiar", "type:ui"]
    }
  }
}
